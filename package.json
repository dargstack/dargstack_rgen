    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "prettier": "3.0.3"
